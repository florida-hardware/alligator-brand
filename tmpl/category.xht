<!DOCTYPE html>
<html lang="en">
<head>
{%init%}
</head>
<body>
<div id="wrap">

{%header%}


<main class="container px-4 px-lg-5">
    <div class="row text-center"><img src="/img/FHFE-Banner2.jpg"/></div>
{%IF1 page.show_h1}
    <div class="row gx-4 gx-lg-5 align-items-center my-5">
        <nav aria-label="breadcrumb"><ol class="breadcrumb">
            {%EACH page.breadcrumb}
                <li class="breadcrumb-item {%IF active}active{%ENDIF}"><a href="{{link}}">{{title}}</a></li>
            {%ENDEACH}
        </ol></nav>
        <h1>{{page.title}}</h1>
    </div>
{%ENDIF1}

<div class="row text-center">
    {%EACH page.category.items}
    {%IF2 active}
    <div class="col-4"><div class="card my-1">
        <a href="{{slug}}.html"><img class="card-img-top" src="/img/prod/{{sku}}.jpg"/></a>
        <div class="card-body"><a href="{{slug}}.html">{{title}}</a></div>
    </div></div>
    {%ENDIF2}
    {%ENDEACH}
</div>

</main>
</div>

{%footer%}
{%exit%}
</body>
</html>